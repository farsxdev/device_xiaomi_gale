# Batterysecret
/(vendor|system/vendor)/bin/batterysecret                                                               u:object_r:batterysecret_exec:s0

# Thermals
/vendor/bin/mi_thermald                                                                                 u:object_r:mi_thermald_exec:s0

# NFC
/dev/nq-nci u:object_r:nfc_device:s0

